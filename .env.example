# Example environment variables for running seed/migrations locally
# Do NOT commit real secrets. Copy to .env and set appropriate values.

# Database (example for local Postgres)
# NETLIFY_DATABASE_URL=postgresql://user:pass@127.0.0.1:5432/dbname?schema=public

# Seed passwords (recommended to set in CI or local .env)
SEED_ADMIN_PASSWORD=change_me_admin
SEED_STAFF_PASSWORD=change_me_staff
SEED_LEAD_PASSWORD=change_me_lead
SEED_CLIENT_PASSWORD=change_me_client

# When true, seed will throw on non-critical errors (fail-fast)
SEED_FAIL_FAST=false
